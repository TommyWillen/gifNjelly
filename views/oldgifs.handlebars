<h2>Vote on previously captioned gifs:</h2>
<ul class="list-group">
    {{#each oldGifs}}
    <li class="list-group-item">
    <p>
        <img src="https://media2.giphy.com/media/{{this.gifId}}/200w.gif?cid=603c5c00zckhvodbm0oxr7brrw3oqsuiughyzh2er22hevs7&rid=200w.gif">
        {{this.caption}}
        <button data-id={{this.id}} class="gifVote">Gif</button>
        <button data-id={{this.id}} class="jellyVote">Jelly</button>
    </p> 
    </li>
    {{/each}}
</ul>

<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
<script src="/js/oldgifs.js">/script>